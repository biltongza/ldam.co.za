{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "node ./generate-last-modified-metadata.js && vite dev",
    "build": "rimraf api/src && node ./generate-last-modified-metadata.js && vite build && copyfiles src/posts/*.md build/server",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "swa": "swa start"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^1.1.7",
    "@microsoft/applicationinsights-web": "^3.1.2",
    "@sveltejs/kit": "^2.5.6",
    "@sveltejs/vite-plugin-svelte": "^3.1.0",
    "@types/glob": "^8.1.0",
    "@types/js-yaml": "^4.0.9",
    "@types/mdast": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "copyfiles": "^2.4.1",
    "dayjs": "^1.11.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.37.0",
    "glob": "^10.3.12",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.3",
    "rehype-external-links": "^3.0.0",
    "rehype-highlight": "^7.0.0",
    "rehype-stringify": "^10.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-gfm": "^4.0.0",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.0",
    "remark-unwrap-images": "^4.0.0",
    "rimraf": "^5.0.5",
    "sass": "^1.75.0",
    "svelte": "^4.2.15",
    "svelte-adapter-azure-swa": "^0.20.0",
    "svelte-check": "^3.6.9",
    "svelte-preprocess": "^5.1.4",
    "to-vfile": "^8.0.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "unified": "^11.0.4",
    "vite": "^5.2.9"
  },
  "type": "module",
  "engines": {
    "node": "18.x"
  }
}
