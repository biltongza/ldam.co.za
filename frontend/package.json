{
  "name": "frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "node ./generate-last-modified-metadata.js && svelte-kit dev --port 5000",
    "build": "node ./generate-last-modified-metadata.js && svelte-kit build",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "swa": "swa start"
  },
  "devDependencies": {
    "@azure/static-web-apps-cli": "^0.8.2",
    "@microsoft/applicationinsights-web": "^2.7.2",
    "@sveltejs/kit": "^1.0.0-next.231",
    "@types/glob": "^7.2.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "dayjs": "^1.10.7",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "glob": "^7.2.0",
    "node-sass": "^7.0.1",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rehype-highlight": "^5.0.1",
    "rehype-stringify": "^9.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "svelte": "^3.46.2",
    "svelte-adapter-azure-swa": "^0.3.0",
    "svelte-check": "^2.3.0",
    "svelte-preprocess": "^4.10.1",
    "to-vfile": "^7.2.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "unified": "^10.1.1"
  },
  "type": "module",
  "dependencies": {
    "@types/js-yaml": "^4.0.5"
  }
}
